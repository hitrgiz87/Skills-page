<Page loaded="pageLoaded">
  <StackLayout  backgroundColor="#E0ACAC">
    <Label text="My Skills" class="text-xl mb-4" style="color: black; font-size: 24;" />
    <ListView id="myListView" itemTap="onItemTap">
      <ListView.itemTemplate>
        <Label class="mx-5" style="color: black; font-size: 20;" id="listItemLabel" />
      </ListView.itemTemplate>
    </ListView>
  </StackLayout>
</Page>

